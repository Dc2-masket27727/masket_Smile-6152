local function getRandomPlayer()
    local players = game.Players:GetPlayers()
    return players[math.random(1, #players)]
end
local Randomplr = getRandomPlayer()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Randomplr.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,2)

else
local function mostrarNotificacion(jugador, mensaje)
    local notificacion = Instance.new("Hint", jugador.PlayerGui)
    notificacion.Text = mensaje
    
    wait(2)
    notificacion:Destroy()
end

local Seeker = game.ReplicatedStorage.GameData.It.Value
local jugador = game.Players.LocalPlayer
local mensaje = "You need Become Seeker to use this!, the Player "..Seeker.." is the server Seeker/It"

mostrarNotificacion(jugador, mensaje)
end
end
end
end)

